{
    "name": "spatie/laravel-email-campaigns-mailgun-feedback",
    "description": "Process feedback for email campaigns sent using Mailgun",
    "keywords": [
        "spatie",
        "laravel-email-campaigns-mailgun-feedback"
    ],
    "homepage": "https://github.com/spatie/laravel-email-campaigns-mailgun-feedback",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/http": "^6.4",
        "spatie/laravel-email-campaigns": "dev-master",
        "spatie/laravel-webhook-client": "^2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailgunFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailgunFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailgunFeedback\\MailgunFeedbackServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Spatie\\MailgunFeedback\\MailgunFeedbackFacade"
            }
        }
    }
}
