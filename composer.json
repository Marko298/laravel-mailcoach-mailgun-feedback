{
    "name": "spatie/laravel-mailcoach-mailgun-feedback",
    "description": "Process feedback for email campaigns sent using Mailgun",
    "keywords": [
        "spatie",
        "laravel-mailcoach-mailgun-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-mailcoach"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/http": "^6.4",
        "orchestra/testbench": "^4.3",
        "spatie/laravel-mailcoach": "^0.9",
        "spatie/laravel-webhook-client": "^2.2"
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachMailgunFeedback\\MailcoachMailgunFeedbackServiceProvider"
            ]
        }
    }
}
